{{>header}}
<div class="signupdiv" style="padding-top: 70px;">
    <div class="createSub" style="box-shadow: 0 0 10px rgba(0,0,0,0.5)">
        <h6 style="text-align: center">All Subscribes</h6>
        <div style="display: inline-flex; margin: 10px; width: 90%">
            <div style="width: 300px; padding-right: 5px"><input type="text" name="find" id="findUser" class="form-control" placeholder="Пошук" ></div>
            <div><button id="button" type="submit" class="btn btn-primary btn-sm" style="height: 30px">Пошук</button></div>
            <div style="margin-left: auto"><button  id="refresh" style="margin-left: 5px" class="btn btn-outline-dark"><svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-arrow-counterclockwise" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                <path fill-rule="evenodd" d="M8 3a5 5 0 1 1-4.546 2.914.5.5 0 0 0-.908-.417A6 6 0 1 0 8 2v1z"/>
                <path d="M8 4.466V.534a.25.25 0 0 0-.41-.192L5.23 2.308a.25.25 0 0 0 0 .384l2.36 1.966A.25.25 0 0 0 8 4.466z"/>
            </svg></button></div>
        </div>
        <div>
            <table class="table">
                <thead class="thead-light">
                <tr>
                    <th scope="col">Subscribes</th>
                    <th scope="col"></th>
                </tr>
                </thead>
                <tbody id="tbody">
                </tbody>
            </table>
        </div>
        <br>
    </div>
</div>
<script src="front/allSubscribes.js"></script>